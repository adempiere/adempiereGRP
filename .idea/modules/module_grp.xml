      <bootclasspath refid="grp.module.bootclasspath"/>
      <classpath refid="grp.module.production.classpath"/>
      <src refid="grp.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.grp"/>
    </javac2>
    
    <copy todir="${grp.output.dir}">
      <fileset dir="${module.grp.basedir}/../../src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.grp.basedir}/../../src/main/scala">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
